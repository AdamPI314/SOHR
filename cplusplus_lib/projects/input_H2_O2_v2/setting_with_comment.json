{
    //"comment": "the keys start with underscore usually are comment of the following key.",
    //"comment": "the number followed by a underscore typically represent a different version of a given key.",
    //"directory_section": "set up some directories.",
    "dir": {
        "include-path": "."
    },
	//"set propagator": "set propagator",
	"propagator":{
		"type": "dlsode"
	},
    //"time_section": "set up some time points.",
    "time": {
        "min_time": 0,
        "max_time": 0.004375545115,
        "critical_time": 0.004375545115,
        "sys_min_time": 1e-10,
        "path_beg_time": 0,
        "path_end_time": 1
    },
    //"chem_init_section": "initial conditions of chemistry.",
    "chem_init": {
        //"pressure_atm": "if it is surface reaction, this pressure means imaginary pressure= CPR, Where R= 8.3144621e7 ergK^-1 mol^-1 in cgs unit.",
        "pressure_atm": 5.54651,
        "init_temperature": 1000,
        "species_index_concentration": {
            "0": 0.3333333333333333,
            "2": 0.6666666666666666
        }
    },
    //"lsode_init_section": "setting of lsode solver.",
    "lsode_init": {
        "dt": 1e-8,
        //"ATOL": "A scalar or array value that specifies the absolute tolerance. The default value is 1.0e-7. Use ATOL = 0.0 (or ATOL[i] = 0.0) for pure relative error control, and use RTOL = 0.0 for pure absolute error control. For an explanation of how to use ATOL and RTOL together, see RTOL below.",
        "atol": 1e-10,
        //"RTOL": "A scalar value that specifies the relative tolerance. The default value is 1.0e-7. Use RTOL = 0.0 for pure absolute error control, and use ATOL = 0.0 (or ATOL[i] = 0.0) for pure relative error control. The estimated local error in the Y[i] argument will be controlled to be less than ewt[i] = RTOL*abs(Y[i]) + ATOL    ; If ATOL is a scalar. ewt[i] = RTOL*abs(Y[i]) + ATOL[i] ; If ATOL is an array. Thus, the local error test passes if, in each component, either the absolute error is less than ATOL (or ATOL[i]), or if the relative error is less than RTOL.",
        "rtol": 1e-10,
        "mf": 22,
        "jt": 2,
        "itask": 1,
        "iopt": 0,
        "itol": 1,
        "istate": 1,
        //"deltaN1": "store to memory or print to file every deltaN1 points before critical temperature/time.",
        "deltaN1": 10,
        //"deltaN2": "store to memory or print to file every deltaN2 points after critical temperature/time.",
        "deltaN2": 1
    },
    //"temperature_section": "temperature information.",
    "T": {
        "critical_temperature": 1100,
        "end_temperature": 1900,
        "target_temperature": 1800
    },
    //"pathway_section": "pathway information.",
    "pathway": {
        "init_spe": 0,
        "atom_followed": "O",
        //"dead_species": "species with no out edge or out rate is zero.",
        "dead_species": [],
        //"trapped_species": "species are trapped in some local reaction with fast conversion rate.",
        "trapped_species": {},
        //"fast_reaction": "local reaction with fast inter-conversion rate.",
        "fast_reaction": {}
    },
    //"srode_init_section": "ShirongODE, abbreviated as srode, attempt to use pathway representation to solve ODEs.",
    "srode_init": {
        //"timeN1": "number of time point before critical time.",
        "timeN1": 100000,
        //"timeN2": "number of time point after critical time.",
        "timeN2": 10,
        //"number2Concentration": "conversion factor from number of species to concentration.",
        "number2Concentration": 0.00003,
        //"iterationN1": "number of inner iterations.",
        "iterationN1": 100000,
        //"iterationN2": "number of outer iterations.",
        "iterationN2": 100
